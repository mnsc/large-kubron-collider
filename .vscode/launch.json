{
    "version": "0.2.0",
      "configurations": [
    {
      "name": "Launch Magnet",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/services/magnet",
      "env": {
        "MAGNET_ID": "2",
        "RING_SIZE": "1000",
        "RING_BASENAME": "magnet",
        "RING_SERVICE": "magnets",
        "EXPERIMENT_URL": "http://experiment-cake:8080/observe",
        "HTTP_ADDR": ":8080"
      }
    },
    {
      "name": "Launch Experiment",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/experiments/cake",
      "env": {
        "HTTP_ADDR": ":8081"
      }
    }
  ]
}